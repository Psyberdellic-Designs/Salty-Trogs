breed [traders trader]

traders-own [
  good-held      ; what's in my sack
  good-produced  ; what I make
  consumption-score  ; how much I've eaten
]

globals [
  demands  ; list of demand probabilities for each good
]
;===========================
; initializers
;===========================

to init-model
  clear-all            ; get rid of everything from last run
  random-seed new-seed ; re-seed random number generator
  reset-ticks          ; reset clock (and plots)
  init-globals         ; initialize globals
  create-traders num-traders
  let circle-radius 10 ; for now
  layout-circle traders circle-radius
  ask patches [init-patch]
  ask traders [init-trader]
end

to init-globals
  let sugar-demand sugar-portion * (1 - salt-demand)
  let spice-demand 1.0 - (salt-demand + sugar-demand)
  set demands (list salt-demand sugar-demand spice-demand)
end

to init-patch
  set pcolor white
end

to init-trader
  set consumption-score 0
  set good-produced random 3
  set shape "Person"

  ifelse good-produced = 0 [
    set color red
  ]
  [
    ifelse good-produced = 1 [
      set color green
    ]
    [
      set color blue
    ]
  ]
end

;===========================
; updaters
;===========================

to update-model
  update-globals
  ask traders [update-trader]
  tick ; advance clock
end

to update-globals
  init-globals
end

to update-trader
  ifelse can-consume? good-held [
    set good-held good-produced
    set consumption-score consumption-score + 1
  ]
  [
    let partner one-of other traders with [can-trade? myself]
    if partner != nobody and can-trade? partner [trade-with partner]
  ]
end

to-report can-trade? [partner]
  ifelse can-consume? [good-held] of partner [
    report true
  ]
  [
    report should-trade? partner  ; I can't consume it, but it may be trade-able later
  ]
end

to trade-with [partner]
  create-link-with partner ; a bit of graphics for show
  ;show "trading"
  let old-good-held good-held
  set good-held [good-held] of partner
  ask partner [set  good-held old-good-held]
  ask link-with partner [die]; get rid of graphics
end

; not sure about this
to-report can-consume? [good]
  report 0 < item good demands
end

; learning algorithm used to decide if a trade
; should be made even though traded good can't
; be consumed.
to-report should-trade? [partner]
  report random 3 < 1 ; for now
end

to-report get-average-score [good]
  let total  0.0
  ask traders with [good-produced = good] [
    set total total + consumption-score
  ]
  let sample-size count  traders with [good-produced = good]
  ifelse sample-size = 0 [
    report 0
  ]
  [
    report (1.0 * total) / sample-size
  ]
end
